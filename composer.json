{
    "name": "luckywindsck/static-reflection",
    "type": "project",
    "require-dev": {
        "cweagans/composer-patches": "1.7.3",
        "phpstan/phpstan": "2.1.18",
        "phpunit/phpunit": "12.2.7",
        "tototoshi/staticmock": "4.0.1"
    },
    "extra": {
        "patches": {
            "tototoshi/staticmock": [
                "patches/tototoshi-staticmock-php84.patch"
